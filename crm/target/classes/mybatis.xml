<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--settings:控制mybatis全局行为-->
    <settings>
        <!--设置mybatis输出日志-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--   设置别名-->
    <typeAliases>
        <package name="com.hu.crm.settings.domain"/>
        <package name="com.hu.crm.workbench.domain"/>
    </typeAliases>
    <mappers>
        <package name="com.hu.crm.settings.dao"/>
        <package name="com.hu.crm.workbench.dao"/>
    </mappers>
</configuration>